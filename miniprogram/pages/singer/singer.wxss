/* miniprogram/pages/singer/singer.wxss */
.singer {
  padding: 30rpx;
}
.singer-item {
  display: flex;
  align-items: center;
  padding: 12rpx 0;
  border-bottom: 1px solid #f5f5f5;
}
.avatar{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.singer-info {
  flex: 1;
  display: flex;
  align-items: center;
}
.arrow {
  color: #5f5c5c;
}