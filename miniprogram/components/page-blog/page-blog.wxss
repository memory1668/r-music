.container {
  /* padding-top: 5rpx; */
  background-color: #f1f1f1;
  height: 100%;
}

.header {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  /* padding-top: 25px; */
  background-color: #fff;
  height: 120rpx;
  position: fixed;
  top: 25px;
  left: 0;
  width: 100%;
  z-index: 99;
  box-sizing: border-box;
}

.header .icon-fabu {
  font-size: 48rpx;
}

.publish-container {
  width: 68rpx;
  height: 68rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 20rpx;
}

.search-container {
  flex: 1;
}

.blog-list {
  margin-top: 151rpx;
}

.blog-panel {
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.block{
  width: 100%;
  height: 48px;
}